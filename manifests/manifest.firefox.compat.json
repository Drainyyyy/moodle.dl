{
  "manifest_version": 2,
  "background": {
    "scripts": ["background.js"],
    "persistent": false
  },
  "browser_action": {
    "default_popup": "popup.html"
  },
  "browser_specific_settings": {
    "gecko": {
      "id": "moodle.download@example.com",
      "strict_min_version": "109.0"
    }
  },
  "permissions": ["downloads", "storage", "activeTab", "notifications", "<all_urls>"]
}
